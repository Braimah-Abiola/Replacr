{
    "name": "Replacr",
    "version": "0.1",
    "description": "An extension by Bhyte!",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "contextMenus"],
    "background": {
        "scripts": ["scripts/main.js"]
    },
    "icons":{
        "16": "icons/16x16.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2 
}